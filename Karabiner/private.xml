<?xml version="1.0"?>
<root>
  <appdef>
    <appname>JAVA</appname>
    <equal>net.java.openjdk.cmd</equal>
  </appdef>

  <item>
    <name>Command + Control + V to pageup</name>
    <identifier>private.my_pageup</identifier>
    <autogen>
      __KeyToKey__
      KeyCode::V, ModifierFlag::CONTROL_L | ModifierFlag::COMMAND_R,
      KeyCode::PAGEUP
    </autogen>
  </item>

  <item>
    <name>Disable all settings while you are using Java.</name>
    <identifier>ignore_apps.java</identifier>
    <only>JAVA</only>
    <autogen>__PassThrough__</autogen>
  </item>
</root>
